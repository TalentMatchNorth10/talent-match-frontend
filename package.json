{
  "name": "talent-match-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "eslint": "npx eslint .",
    "eslint:fix": "npx eslint --fix .",
    "prepare": "husky",
    "openapi:clean": "rimraf ./libs/openapi/client/src",
    "openapi:client": "npm run openapi:clean && openapi-generator-cli generate --generator-key client-server",
    "openapi:local": "npm run openapi:clean && openapi-generator-cli generate --generator-key client-local",
    "openapi:file": "npm run openapi:clean && openapi-generator-cli generate --generator-key client-file"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@fullcalendar/angular": "^6.1.10",
    "@fullcalendar/core": "^6.1.11",
    "@fullcalendar/daygrid": "^6.1.11",
    "@fullcalendar/interaction": "^6.1.11",
    "@fullcalendar/timegrid": "^6.1.14",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "prettier-eslint": "^16.3.0",
    "quill": "^2.0.2",
    "rimraf": "^5.0.7",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.8.1",
    "swiper": "^11.1.4",
    "tslib": "^2.3.0",
    "uuid": "^9.0.1",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.6",
    "@angular/cli": "^17.3.6",
    "@angular/compiler-cli": "^17.3.0",
    "@eslint/js": "^9.1.1",
    "@types/jasmine": "~5.1.0",
    "@types/quill": "^1.3.10",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-tailwindcss": "^3.17.0",
    "globals": "^15.1.0",
    "husky": "^9.0.11",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.3.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.1",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.2",
    "typescript-eslint": "^7.8.0"
  },
  "hooks": {
    "pre-commit": "npm run eslint:fix"
  }
}
